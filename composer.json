{
    "name": "jesperoestergaardjensen/photocentral-synology-storage-server",
    "description": "Synology NAS storage for PhotoCentral, server",
    "license": "gpl-3.0",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": "^7.4",
        "jesperoestergaardjensen/photocentral-storage" : "dev-main",
        "jesperoestergaardjensen/linux-file-system-helper": "v1.0.0-beta.3",
        "jesperoestergaardjensen/linux-image-helper" : "v0.1.3",
        "texlab/mydb": "^0.0.6",
        "mindplay/unbox": "2.0.4",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "PhotoCentralSynologyStorageServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoCentralSynologyStorageServer\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "9.2.x-dev"
    },
    "scripts": {
        "test": "phpunit"
    }
}
