{
    "name": "jesperoestergaardjensen/photocentral-synology-storage-server",
    "description": "Synology NAS storage for PhotoCentral, server",
    "license": "gpl-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=7.4",
        "jesperoestergaardjensen/photocentral-storage" : "^v1.0.1",
        "jesperoestergaardjensen/linux-file-system-helper": "^v1.0.0",
        "jesperoestergaardjensen/linux-image-helper" : "^v1.0.0",
        "jesperoestergaardjensen/mydb": "dev-master",
        "slince/di" : "^3.2.0",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "PhotoCentralSynologyStorageServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoCentralSynologyStorageServer\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "9.2.x-dev",
        "jesperoestergaardjensen/exif-samples": "1.0.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
